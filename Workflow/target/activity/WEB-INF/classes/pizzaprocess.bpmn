  <process id="pizzaprocess" name="Pizza process" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <scriptTask id="scripttask1" name="Valid pizza order" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>    out:print "Validating pizza order for " + customerName + "\n";
        if(amount &gt; 10){
          rejectReason = "That is a little bit too much for us " + amount + " pizzas";
          pizzaOrderToOven = false;
        } else {
          pizzaOrderToOven = true;
        }
        out:print "Pizza order was validated with outcome " + pizzaOrderToOven + "\n";
        if(pizzaOrderToOven == false) {
          out:print "Reject reason is " + rejectReason + "\n";
        } else {
          out:print amount + " " + pizza + " pizzas going to the oven\n";
        }		</script>
    </scriptTask>
    <sequenceFlow id="flow2" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="scripttask1" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Calculate prize" activiti:class="com.activity.PizzaService"></serviceTask>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="servicetask1"></sequenceFlow>
    <userTask id="selvi" name="Bake the Pizza" activiti:assignee="selvi"></userTask>
    <sequenceFlow id="flow5" sourceRef="servicetask1" targetRef="selvi"></sequenceFlow>
    <intermediateCatchEvent id="timerintermediatecatchevent1" name="TimerCatchEvent">
      <timerEventDefinition>
        <timeDuration>PT1M</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow6" sourceRef="selvi" targetRef="timerintermediatecatchevent1"></sequenceFlow>
    <userTask id="manager" name="Manager will bake the pizza" activiti:async="true" activiti:assignee="manager"></userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="manager" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway1" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="timerintermediatecatchevent1" targetRef="manager"></sequenceFlow>
  </process>
